    public PurchaseDatailsFrame() {
        initComponents();
        fillPurchaseDetailsTable();
        fillProductCombo();
        fillPurchaseCombo();
        deleteButton.setEnabled(false);
        updateButton.setEnabled(false);
    }
    Product product = new Product();
    ProductDao productDao = new ProductDaoImpl();
    Purchase purchase = new Purchase();
    PurchaseDao purchaseDao = new PurchaseDaoImpl();
    PurchaseDetails details = new PurchaseDetails();
    PurchaseDetailDao purchaseDetailsDao = new PurchaseDetailsDaoImpl();

    public void fillPurchaseDetailsTable() {
        Object columnAdd[] = {"Purchase Details Id", "Purchase Name", "Total", "Product Name", "Quantity", "Purchase Price", "Created Date", "Created By", "Modified Date", "Modified By", "Status"};
        DefaultTableModel model = new DefaultTableModel(columnAdd, 0);
        List<PurchaseDetails> list = purchaseDetailsDao.viewAllRecord();
        for (PurchaseDetails v : list) {
            product = productDao.viewById(v.getProduct().getProductId());
            purchase = purchaseDao.viewById(v.getPurchase().getPurchaseId());
            Object rowAdd[] = {v.getPurchaseDetailsId(), purchase.getPurchaseCode(), v.getTotal(), product.getProductName(), v.getQuantity(), v.getPurchasePrice(), v.getCreatedDate(), v.getCreatedName(), v.getModifiedDate(), v.getModifedName(), v.getStatus()};
            model.addRow(rowAdd);
            detailsTable.setModel(model);
        }
    }

    public void fillProductCombo() {
        List<Product> list = productDao.viewAllRecord();
        productCombo.addItem("--- Select Product ---");
        for (Product b : list) {
            productCombo.addItem(b.getProductName());
        }
    }

    public void fillPurchaseCombo() {
        List<Purchase> list = purchaseDao.viewAllRecord();
        purchaseCombo.addItem("--- Select Purchase ---");
        for (Purchase b : list) {
            purchaseCombo.addItem(b.getPurchaseCode());
        }
    }

