    private void searchTextKeyReleased(java.awt.event.KeyEvent evt) {                                       
        DefaultTableModel table = (DefaultTableModel) detailsTable.getModel();
        String search = searchText.getText();
        TableRowSorter<DefaultTableModel> trs = new TableRowSorter<DefaultTableModel>(table);
        detailsTable.setRowSorter(trs);
        trs.setRowFilter(RowFilter.regexFilter(search));
    }                                      

    private void addButtonActionPerformed(java.awt.event.ActionEvent evt) {                                          
        if (totalText.getText().trim().isEmpty() || quantityText.getText().trim().isEmpty()
                || priceTex.getText().trim().isEmpty() || statusText.getText().trim().isEmpty()) {
            JOptionPane.showMessageDialog(this, "Field(s) are Empty");
        } else {
            String productName = productCombo.getSelectedItem().toString();
            Integer productId = productDao.viewByName(productName);
            String purchaseName = purchaseCombo.getSelectedItem().toString();
            Integer purchaseId = purchaseDao.viewByName(purchaseName);
            purchase.setPurchaseId(purchaseId);
            product.setProductId(productId);
            details.setProduct(product);
            details.setPurchase(purchase);
            String total = totalText.getText();
            String quantity = quantityText.getText();
            String price = priceTex.getText();
            String status = statusText.getText();
            details.setTotal(total);
            details.setStatus(status);
            details.setQuantity(quantity);
            details.setPurchasePrice(price);
            Integer number = purchaseDetailsDao.addPurchaseDetails(details);
            if (number > 0) {
                JOptionPane.showMessageDialog(this, "Added Successfully");
                fillPurchaseDetailsTable();
                purchaseCombo.setSelectedIndex(0);
                productCombo.setSelectedIndex(0);
                totalText.setText("");
                quantityText.setText("");
                priceTex.setText("");
                statusText.setText("");
            }
        }
    }                                         

    private void updateButtonActionPerformed(java.awt.event.ActionEvent evt) {                                             
        if (totalText.getText().trim().isEmpty() || quantityText.getText().trim().isEmpty()
                || priceTex.getText().trim().isEmpty() || statusText.getText().trim().isEmpty()) {
            JOptionPane.showMessageDialog(this, "Field(s) are Empty");
        } else {
            Integer id = (Integer) detailsTable.getValueAt(detailsTable.getSelectedRow(), 0);
            String productName = productCombo.getSelectedItem().toString();
            Integer productId = productDao.viewByName(productName);
            String purchaseName = purchaseCombo.getSelectedItem().toString();
            Integer purchaseId = purchaseDao.viewByName(purchaseName);
            purchase.setPurchaseId(purchaseId);
            product.setProductId(productId);
            details.setProduct(product);
            details.setPurchase(purchase);
            String total = totalText.getText();
            String quantity = quantityText.getText();
            String price = priceTex.getText();
            String status = statusText.getText();
            details.setTotal(total);
            details.setStatus(status);
            details.setQuantity(quantity);
            details.setPurchasePrice(price);
            details.setPurchaseDetailsId(id);
            Integer number = JOptionPane.showConfirmDialog(this, "Do You Want Update your Record?");
            if (number == 0) {
                purchaseDetailsDao.updatePurchaseDetails(details);
                fillPurchaseDetailsTable();
                JOptionPane.showMessageDialog(this, "Updated Successfully");
                purchaseCombo.setSelectedIndex(0);
                productCombo.setSelectedIndex(0);
                totalText.setText("");
                quantityText.setText("");
                priceTex.setText("");
                statusText.setText("");
            }
        }
    }                                            

    private void deleteButtonActionPerformed(java.awt.event.ActionEvent evt) {                                             
        Integer id = (Integer) detailsTable.getValueAt(detailsTable.getSelectedRow(), 0);
        Integer number = JOptionPane.showConfirmDialog(this, "Do You Want Delete your Record?");
        if (number == 0) {
            purchaseDetailsDao.deletePurchaseDetails(id);
            fillPurchaseDetailsTable();
            JOptionPane.showMessageDialog(this, "Delete Successfully");
            purchaseCombo.setSelectedIndex(0);
            productCombo.setSelectedIndex(0);
            totalText.setText("");
            quantityText.setText("");
            priceTex.setText("");
            statusText.setText("");
        }
    }                                            

    private void clearButtonActionPerformed(java.awt.event.ActionEvent evt) {                                            
        deleteButton.setEnabled(false);
        updateButton.setEnabled(false);
        addButton.setEnabled(true);
        purchaseCombo.setSelectedIndex(0);
        productCombo.setSelectedIndex(0);
        totalText.setText("");
        quantityText.setText("");
        priceTex.setText("");
        statusText.setText("");
    }                                           

    private void detailsTableMouseClicked(java.awt.event.MouseEvent evt) {                                          
        deleteButton.setEnabled(true);
        updateButton.setEnabled(true);
        addButton.setEnabled(false);
        clearButton.setEnabled(true);
        Integer id = (Integer) detailsTable.getValueAt(detailsTable.getSelectedRow(), 0);
        PurchaseDetails p = purchaseDetailsDao.viewById(id);
        product = productDao.viewById(p.getProduct().getProductId());
        productCombo.setSelectedItem(product.getProductName());
        purchase = purchaseDao.viewById(p.getPurchase().getPurchaseId());
        purchaseCombo.setSelectedItem(purchase.getPurchaseCode());
        totalText.setText(p.getTotal());
        quantityText.setText(p.getQuantity());
        priceTex.setText(p.getPurchasePrice());
        statusText.setText(p.getStatus());
    }                                         

    private void addButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        try {
            JasperDesign design = JRXmlLoader.load("D:\\University_work\\TuitionWork\\IReport\\ShopManagementSystem\\PurchaseDetails.jrxml");
            JasperReport report = JasperCompileManager.compileReport(design);
            JasperPrint jasperPrint = JasperFillManager.fillReport(report, null, con);
            JasperViewer.viewReport(jasperPrint);
        } catch (JRException ex) {
            ex.printStackTrace();
        }
    }                                          

