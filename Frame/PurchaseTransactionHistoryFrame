    Purchase purchase = new Purchase();
    PurchaseDao purchaseDao = new PurchaseDaoImpl();
    PurchaseTranscationHistoryDao dao = new PurchaseTransactionHistoryDaoImpl();
    PurchaseTranscationHistory history = new PurchaseTranscationHistory();
    Account a = new Account();
    AccountDao accountDao = new AccountDaoImpl();
    
    public void fillPurchaseTransactionHistoryTable() {
        Object columnAdd[] = {"Transaction Id", "Account", "Purchase Date", "Purchase", "Transaction Amount", "Remarks", "Created Date", "Created By", "Modified Date", "Modified By", "Status"};
        DefaultTableModel model = new DefaultTableModel(columnAdd, 0);
        List<PurchaseTranscationHistory> list = dao.viewAllRecord();
        for (PurchaseTranscationHistory v : list) {
            purchase = purchaseDao.viewById(v.getPurchase().getPurchaseId());
            a = accountDao.viewById(v.getAccount().getAccountId());
            Object rowAdd[] = {v.getPurcchaseTranscationHistoryId(), a.getAccountTitle(), v.getPurchaeDate(), purchase.getPurchaseCode(), v.getTranscationAmount(), v.getRemarks(), v.getCreatedDate(), v.getCreatedName(), v.getModifiedDate(), v.getModifedName(), v.getStatus()};
            model.addRow(rowAdd);
            historyTable.setModel(model);
        }
    }
    
    public void fillAccountCombo() {
        List<Account> list = accountDao.viewAllRecord();
        accountCombo.addItem("--- Select Account ---");
        for (Account b : list) {
            accountCombo.addItem(b.getAccountTitle());
        }
    }
    
    public void fillPurchaseCombo() {
        List<Purchase> list = purchaseDao.viewAllRecord();
        purchaseCombo.addItem("--- Select Purchase ---");
        for (Purchase b : list) {
            purchaseCombo.addItem(b.getPurchaseCode());
        }
    }

