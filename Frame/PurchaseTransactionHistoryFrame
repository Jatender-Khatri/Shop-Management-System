    private void homeButtonActionPerformed(java.awt.event.ActionEvent evt) {                                           
        HomeFrame frame = new HomeFrame();
        frame.setVisible(true);
        this.dispose();
    }                                          

    private void searchTextKeyReleased(java.awt.event.KeyEvent evt) {                                       
        DefaultTableModel table = (DefaultTableModel) historyTable.getModel();
        String search = searchText.getText();
        TableRowSorter<DefaultTableModel> trs = new TableRowSorter<DefaultTableModel>(table);
        historyTable.setRowSorter(trs);
        trs.setRowFilter(RowFilter.regexFilter(search));
    }                                      

    private void addButtonActionPerformed(java.awt.event.ActionEvent evt) {                                          
        if (amountText.getText().trim().isEmpty() || remarksText.getText().trim().isEmpty()
                || statusText.getText().trim().isEmpty()) {
            JOptionPane.showMessageDialog(this, "Field(s) are Empty");
        } else {
            String purchaseName = purchaseCombo.getSelectedItem().toString();
            Integer purchaseId = purchaseDao.viewByName(purchaseName);
            String accountName = accountCombo.getSelectedItem().toString();
            Integer accountId = accountDao.viewByName(accountName);
            String amount = amountText.getText();
            String remarks = remarksText.getText();
            String status = statusText.getText();
            a.setAccountId(accountId);
            purchase.setPurchaseId(purchaseId);
            history.setPurchase(purchase);
            history.setAccount(a);
            history.setTranscationAmount(amount);
            history.setRemarks(remarks);
            history.setStatus(status);
            history.setPurchaeDate(dateText.getDate());
            Integer number = dao.addPurchaseTranscationHistory(history);
            if (number > 0) {
                JOptionPane.showMessageDialog(this, "Added Successfully");
                fillPurchaseTransactionHistoryTable();
                purchaseCombo.setSelectedIndex(0);
                accountCombo.setSelectedIndex(0);
                dateText.setDate(null);
                amountText.setText("");
                remarksText.setText("");
                statusText.setText("");
            }
        }

    }                                         

    private void updateButtonActionPerformed(java.awt.event.ActionEvent evt) {                                             
        if (amountText.getText().trim().isEmpty() || remarksText.getText().trim().isEmpty()
                || statusText.getText().trim().isEmpty()) {
            JOptionPane.showMessageDialog(this, "Field(s) are Empty");
        } else {
            Integer id = (Integer) historyTable.getValueAt(historyTable.getSelectedRow(), 0);
            String purchaseName = purchaseCombo.getSelectedItem().toString();
            Integer purchaseId = purchaseDao.viewByName(purchaseName);
            String accountName = accountCombo.getSelectedItem().toString();
            Integer accountId = accountDao.viewByName(accountName);
            String amount = amountText.getText();
            String remarks = remarksText.getText();
            String status = statusText.getText();
            a.setAccountId(accountId);
            purchase.setPurchaseId(purchaseId);
            history.setPurchase(purchase);
            history.setAccount(a);
            history.setTranscationAmount(amount);
            history.setRemarks(remarks);
            history.setStatus(status);
            history.setPurchaeDate(dateText.getDate());
            history.setPurcchaseTranscationHistoryId(id);
            Integer number = JOptionPane.showConfirmDialog(this, "Do You Want Update Your Record?");
            if (number == 0) {
                dao.updatePurchaseTranscationHistory(history);
                fillPurchaseTransactionHistoryTable();
                JOptionPane.showMessageDialog(this, "Updated Successfully");
                deleteButton.setEnabled(false);
                updateButton.setEnabled(false);
                addButton.setEnabled(true);
                clearButton.setEnabled(true);
                purchaseCombo.setSelectedIndex(0);
                accountCombo.setSelectedIndex(0);
                dateText.setDate(null);
                amountText.setText("");
                remarksText.setText("");
                statusText.setText("");
            }
        }
    }                                            

    private void deleteButtonActionPerformed(java.awt.event.ActionEvent evt) {                                             
        Integer id = (Integer) historyTable.getValueAt(historyTable.getSelectedRow(), 0);
        Integer number = JOptionPane.showConfirmDialog(this, "Do You Want Update Your Record?");
        if (number == 0) {
            dao.deletePurchaseTranscationHistory(id);
            fillPurchaseTransactionHistoryTable();
            JOptionPane.showMessageDialog(this, "Delete Successfully");
            purchaseCombo.setSelectedIndex(0);
            accountCombo.setSelectedIndex(0);
            dateText.setDate(null);
            amountText.setText("");
            remarksText.setText("");
            statusText.setText("");
        }
    }                                            

    private void clearButtonActionPerformed(java.awt.event.ActionEvent evt) {                                            
        deleteButton.setEnabled(false);
        updateButton.setEnabled(false);
        addButton.setEnabled(true);
        clearButton.setEnabled(true);
        purchaseCombo.setSelectedIndex(0);
        accountCombo.setSelectedIndex(0);
        dateText.setDate(null);
        amountText.setText("");
        remarksText.setText("");
        statusText.setText("");
    }                                           

    private void historyTableMouseClicked(java.awt.event.MouseEvent evt) {                                          
        deleteButton.setEnabled(true);
        updateButton.setEnabled(true);
        addButton.setEnabled(false);
        clearButton.setEnabled(false);
        Integer id = (Integer) historyTable.getValueAt(historyTable.getSelectedRow(), 0);
        PurchaseTranscationHistory pth = dao.viewById(id);
        purchase = purchaseDao.viewById(pth.getPurchase().getPurchaseId());
        purchaseCombo.setSelectedItem(purchase.getPurchaseCode());
        a = accountDao.viewById(pth.getAccount().getAccountId());
        accountCombo.setSelectedItem(a.getAccountTitle());
        dateText.setDate(pth.getPurchaeDate());
        amountText.setText(pth.getTranscationAmount());
        remarksText.setText(pth.getRemarks());
        statusText.setText(pth.getStatus());
    }                                         

    private void addButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        try {
            JasperDesign design = JRXmlLoader.load("D:\\University_work\\TuitionWork\\IReport\\ShopManagementSystem\\Jatender.jrxml");
            JasperReport report = JasperCompileManager.compileReport(design);
            JasperPrint jasperPrint = JasperFillManager.fillReport(report, null, con);
            JasperViewer.viewReport(jasperPrint);
        } catch (JRException ex) {
            ex.printStackTrace();
        }
    }                                          

