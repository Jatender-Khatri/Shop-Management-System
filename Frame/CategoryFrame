    private void homeButtonActionPerformed(java.awt.event.ActionEvent evt) {                                           
        HomeFrame frame = new HomeFrame();
        frame.setVisible(true);
        this.dispose();
    }                                          

    private void searchTextKeyReleased(java.awt.event.KeyEvent evt) {                                       
        DefaultTableModel table = (DefaultTableModel) categoryTable.getModel();
        String search = searchText.getText();
        TableRowSorter<DefaultTableModel> trs = new TableRowSorter<DefaultTableModel>(table);
        categoryTable.setRowSorter(trs);
        trs.setRowFilter(RowFilter.regexFilter(search));
    }                                      

    private void addButtonActionPerformed(java.awt.event.ActionEvent evt) {                                          
        if (categoryText.getText().trim().isEmpty() || reamrksText.getText().trim().isEmpty() || statusText.getText().trim().isEmpty()) {
            JOptionPane.showMessageDialog(this, "Field(s) are Empty");
        } else {
            String category = categoryText.getText();
            String remarks = reamrksText.getText();
            String status = statusText.getText();
            c.setCategory(category);
            c.setRemarks(remarks);
            c.setStatus(status);
            int number = categoryDao.addCategory(c);
            if (number > 0) {
                JOptionPane.showMessageDialog(this, "Added Successfully");
                fillCategoryTable();
                categoryText.setText("");
                reamrksText.setText("");
                statusText.setText("");
            }
        }
    }                                         

    private void updateButtonActionPerformed(java.awt.event.ActionEvent evt) {                                             
        if (categoryText.getText().trim().isEmpty() || reamrksText.getText().trim().isEmpty() || statusText.getText().trim().isEmpty()) {
            JOptionPane.showMessageDialog(this, "Field(s) are Empty");
        } else {
            Integer categoryId = (Integer) categoryTable.getValueAt(categoryTable.getSelectedRow(), 0);
            String category = categoryText.getText();
            String remarks = reamrksText.getText();
            String status = statusText.getText();
            c.setCategory(category);
            c.setRemarks(remarks);
            c.setStatus(status);
            c.setCategoryId(categoryId);
            int number = JOptionPane.showConfirmDialog(this, "Do You Want Update your Record?");
            if (number == 0) {
                categoryDao.updateCategory(c);
                fillCategoryTable();
                JOptionPane.showMessageDialog(this, "Update Successfully");
                categoryText.setText("");
                reamrksText.setText("");
                statusText.setText("");
            }
        }
    }                                            

    private void deleteButtonActionPerformed(java.awt.event.ActionEvent evt) {                                             
        Integer categoryId = (Integer) categoryTable.getValueAt(categoryTable.getSelectedRow(), 0);
        int number = JOptionPane.showConfirmDialog(this, "Do You Want Delete Your Record?");
        if (number == 0) {
            categoryDao.deleteCategory(categoryId);
            fillCategoryTable();
            JOptionPane.showMessageDialog(this, "Delete Successfully");
            categoryText.setText("");
            reamrksText.setText("");
            statusText.setText("");
        }
    }                                            

    private void clearButtonActionPerformed(java.awt.event.ActionEvent evt) {                                            
        updateButton.setEnabled(true);
        deleteButton.setEnabled(true);
        categoryText.setText("");
        reamrksText.setText("");
        statusText.setText("");
    }                                           

    private void categoryTableMouseClicked(java.awt.event.MouseEvent evt) {                                           
        updateButton.setEnabled(true);
        deleteButton.setEnabled(true);
        Integer categoryId = (Integer) categoryTable.getValueAt(categoryTable.getSelectedRow(), 0);
        Category category = categoryDao.viewById(categoryId);
        categoryText.setText(category.getCategory());
        reamrksText.setText(category.getRemarks());
        statusText.setText(category.getStatus());
    }                                          

    private void addButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                           
       try {
            JasperDesign design = JRXmlLoader.load("D:\\University_work\\TuitionWork\\IReport\\ShopManagementSystem\\Category.jrxml");
            JasperReport report = JasperCompileManager.compileReport(design);
            JasperPrint jasperPrint = JasperFillManager.fillReport(report, null, con);
            JasperViewer.viewReport(jasperPrint);
        } catch (JRException ex) {
            ex.printStackTrace();
        }
    }                                          

