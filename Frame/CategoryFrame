    Category c = new Category();
    CategoryDao categoryDao = new CategoryDaoImpl();

    public void fillCategoryTable() {
        Object columnAdd[] = {"Category Id", "Category", "Remarks", "Created Date", "Created By", "Modified Date", "Modified By", "Status"};
        DefaultTableModel model = new DefaultTableModel(columnAdd, 0);
        List<Category> list = categoryDao.viewAllRecord();
        for (Category v : list) {
            Object rowAdd[] = {v.getCategoryId(), v.getCategory(), v.getRemarks(), v.getCreatedDate(), v.getCreatedName(), v.getModifiedDate(), v.getModifedName(), v.getStatus()};
            model.addRow(rowAdd);
            categoryTable.setModel(model);
        }
    }

